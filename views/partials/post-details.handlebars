{{!-- {{#each posts as |post|}} --}}
<div class="card" style="width: 18rem;">

  <div class="card-body">
    <a href="/post/{{id}}">
      <h5 class="card-title">Title: {{post_title}}</h5>
    </a>
    {{!-- <h6 class="card-subtitle mb-2 text-muted">{{post_author}}</h6> --}}
    <p class="card-text">{{post_contents}}</p>
  </div>

</div>
  {{!-- {{/each}} --}}

